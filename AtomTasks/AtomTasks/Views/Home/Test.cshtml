
@{
    ViewBag.Title = "Test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Test</h2>

<table>
@foreach (var item in ViewBag.Tasks)
{
    <tr>
        <td>
            @item.CustomerObj.Login
        </td>
        <td>
            @item.TaskCategoryObj.Code
        </td>
        <td>
            @item.TaskStatusObj.Code
        </td>
        <td>
            @item.UserMarkObj.Text
        </td>
        <td>
            @item.Name
        </td>
        <td>
            @item.Description
        </td>
        <td>
            @item.StartPrice
        </td>
        <td>
            @item.Date
        </td>
        <td>
            @item.LocationX
        </td>
        <td>
            @item.LocationY
        </td>
    </tr>
}

</table>
